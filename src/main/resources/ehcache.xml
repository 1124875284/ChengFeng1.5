<ehcache>

    <!--缓存持久化硬盘策略为临时目录-->
    <diskStore path="java.io.tmpdir" />


    <!--指定缓存名称、缓存对象的大小上限、设置缓存可以失效、指定缓存Idle和Live状态下的存活时间、指定缓存可以刷到硬盘上-->
    <cache name="chengfengCache" maxElementsInMemory="10000" eternal="false"
           timeToIdleSeconds="120" timeToLiveSeconds="600" overflowToDisk="true" />

</ehcache>